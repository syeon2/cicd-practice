# top-most EditorConfig file
root = true

[*]
# [encoding-utf8]
charset = utf-8

# [newline-eof]
end_of_line = lf

# [newline-eof]
insert_final_newline = true

[*.bat]
end_of_line = crlf

[*.java]
# [indentation-tab]
indent_style = tab

# [4-space-tab]
indent_size = 4
tab_width = 4

# [no-trailing-space]
trim_trailing_whitespace = true

[line-length-120]
max_line_length = 120
